{
  "name": "vuex-reference-helper",
  "displayName": "Vuex Reference Helper",
  "description": "VSCode extension that enables Ctrl/Cmd+Click navigation to Vuex state, getters, and mutations across .vue, .js, and .ts files with alias and path resolution support.",
  "version": "2.2.5",
  "publisher": "qjsrodksro",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript"
      },
      {
        "id": "typescript"
      },
      {
        "id": "vue"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "pretest": "rm -rf ./out && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.99.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "sinon": "^21.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.8.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@vue/compiler-sfc": "^3.5.18"
  },
  "license": "MIT",
  "repository": "github:mochang2/vuex-reference-helper"
}
